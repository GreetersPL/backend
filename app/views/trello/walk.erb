- <%=Walk.human_attribute_name('name')%>: *<%=@walk.name%>*
- <%=Walk.human_attribute_name('email')%>: *<%=@walk.email%>*
- <%=Walk.human_attribute_name('dates')%>:<% @walk.dates.each do |date_choosen| %>
   * <%= date_choosen['date'] %>: *<%= date_choosen['from'] %>* - *<%= date_choosen['to'] %>*
<% end %>
- <%=Walk.human_attribute_name('languages')%>: <% @walk.languages.each do |lang| %>
    * *<%= lang['language'] %>* - *<%= I18n.t("langs.levels.#{lang['level']}") %>*
<% end %>
- <%=Walk.human_attribute_name('user_lang')%>: *<%=I18n.t("langs.#{@walk.user_lang}")%>*
- <%=Walk.human_attribute_name('created_at')%>: *<%= l @walk.created_at%>*
